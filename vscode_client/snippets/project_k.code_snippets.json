{
    "Navigate": {
		"prefix": ["navigate url"],
		"body": [
			"navigate ${1:url}\n"
		],
		"description": "Load a new web page in the current browser window."
	},
	"get": {
		"prefix":["click element"],
		"body":["click ${1:element}\n"]
	},
	"if":{
		"prefix":["if"],
		"body":["if ${1:codition} {","\t$2","}$0\n"]
	},
	"else if": {
		"prefix":["else if"],
		"body":["else if ${1:codition} {","\t$2","}$0\n"]
	},
	"else": {
		"prefix":["else"],
		"body":["else {","\t$1","}$0\n"]
	}
}